<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aplicativo.diseasedetector">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.DiseaseDetector">
        <activity
            android:name="com.aplicativo.diseasedetector.BlackSpotActivity"
            android:exported="false" />
        <activity
            android:name="com.aplicativo.diseasedetector.GreeningActivity"
            android:exported="false" />
        <activity
            android:name="com.aplicativo.diseasedetector.CankerActivity"
            android:exported="false" />
        <activity
            android:name="com.aplicativo.diseasedetector.HealtyActivity"
            android:exported="false" />
        <activity
            android:name="com.aplicativo.diseasedetector.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>